<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8"/>
        <title>{% block title %}{% endblock %}</title>
        <!-- Mobile viewport optimisation -->
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <!-- (en) Add your meta data here -->
        <!-- (de) Fuegen Sie hier ihre Meta-Daten ein -->

        {% block stylesheets %}
            <link href="{{ asset('css/admin.css') }}" rel="stylesheet" />
        {% endblock %}
        {% block additionalstylesheets %}

        {% endblock %}


        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>

    <body>
        {% include 'admin/include/header.html.twig' %}
        {% include 'admin/include/navigation.html.twig' %}

        <div class="container">

            <div class="main">
                <div class="page-title">
                    <h1 style="padding-top:40px; padding-left:10px; font-family: RobotoCondensedRegular;">{% block pagetitle %}{% endblock %}</h1>
                </div>

                {% block body %}

                {% endblock %}
            </div>

        </div><!-- /.container -->

        <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
        <!-- Include all compiled plugins (below), or include individual files as needed -->
        <script src="/js/bootstrap.min.js"></script>
        <script src="/js/custom.js"></script>
        {% block javascripts %}

        {% endblock %}

        {% block bodyoverlay %}

        {% endblock %}

    </body>
</html>
