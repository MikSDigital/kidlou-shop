{% extends 'admin/base.html.twig' %}
{% block title %} Admin Area {% endblock %}
{% block stylesheets %}
    {{ parent() }}
    <link href="/css/jquery.contextMenu.css" rel="stylesheet" />
    <link href="/css/jquery-ui.css" rel="stylesheet" />
{% endblock %}
{% block pagetitle %}{{ 'Delivers' | trans }}{% endblock %}
{% block body %}
    <section class="box info admin">
        <div class="row">
            <div class="col-md-3"></div>
            <div class="col-md-6" id="admin-deliver-update" data-url="{{ path('admin_deliver_exception_update') }}">
                <div class="calendar-dates-show">{{ calendar.getHtmlCalendar() | raw }}</div>
            </div>
            <div class="col-md-3"></div>
        </div>
    </section>
{% endblock %}
{% block bodyoverlay %}
    <div id="bodyoverlay">
        <div id="bodyoverlay-box"></div>
    </div>
{% endblock %}
{% block javascripts %}
    <script src="{{ asset('js/calendar.js') }}"></script>
    <script src="{{ asset('js/admin/calendar.js') }}"></script>
{% endblock %}
